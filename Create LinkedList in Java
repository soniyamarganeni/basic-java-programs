import java.io.*;
public class collection
{
    public static void main(String args[])
    {
        LinkedList<String> a=new LinkedList<String>();
        a.add("Dog");
         a.add("Cow");
        a.add("Cat");
        System.out.println("LinkedList: "+a);
    }
}//Code here
